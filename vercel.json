{
  "version": 2,
  "builds": [
    {
      "src": "sfeed_project/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static_build",
      "config": { "distDir": "staticfiles_build"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sfeed_project/wsgi.py"
    },
    {
      "src": "/static(.*)",
      "dest": "/static/$1"
    }
  ]
}
